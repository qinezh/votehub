{
  "name": "votehub",
  "version": "0.1.0",
  "main": "index.js",
  "repository": {},
  "author": "qinezh",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts",
    "serve": "nodemon dist/app.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "predeploy": "npm run build && cp package.json dist",
    "deploy": "gh-pages -d dist -b deploy -m Newdeployment"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "jsonwebtoken": "^7.4.3",
    "mongoose": "^4.11.5",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.16.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^4.0.0",
    "@types/errorhandler": "^0.0.30",
    "@types/express": "^4.0.36",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/mongoose": "^4.7.19",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.19",
    "@types/passport": "^0.3.3",
    "@types/passport-github": "^1.1.1",
    "gh-pages": "^1.0.0",
    "nodemon": "^1.11.0"
  }
}
